{
    "manifest_version": 3,
    "name": "RemoveBP",
    "version": "1.0",
    "description": "Remove /?bp from the YouTube homepage",
    "permissions": [
        "webNavigation"
      ],
    "action": {
      "default_icon": "icon.png",
      "default_popup": "settings.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  